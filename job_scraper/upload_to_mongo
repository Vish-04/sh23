import pymongo

# Replace these with your MongoDB Atlas connection details
mongodb_url = "your_mongodb_atlas_connection_url"
database_name = "your_database_name"
collection_name = "your_collection_name"

# Sample list of JSON objects
json_objects = [
    {"name": "John", "age": 30},
    {"name": "Alice", "age": 25},
    {"name": "Bob", "age": 35},
]

# Connect to MongoDB Atlas
client = pymongo.MongoClient(mongodb_url)

# Access the desired database
db = client[database_name]

# Access the desired collection (creates it if it doesn't exist)
collection = db[collection_name]

# Insert the JSON objects into the collection
collection.insert_many(json_objects)

# Close the MongoDB connection
client.close()
